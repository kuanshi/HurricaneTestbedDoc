{
  "Name": "rWHALE_",
  "Author": "Adam Zsarn√≥czay",
  "WorkflowType": "Parametric Study",
  "runDir": "...",
  "localAppDir": "C:/RDT/",
  "units": {
      "force": "kips",
      "length": "in",
      "time": "sec"
   },
   "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": false
  },
  "Applications": {
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "Min": "1",
        "Max": "2",
        "buildingSourceFile":"input_params.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "records/EventGrid.csv",
        "samples": 5,
        "neighbors": 4
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {
        "pathEventData": "records/"
      }
    }],
    "Modeling": {
      "Application": "OpenSeesPyInput",
      "ApplicationData": {
        "mainScript": "cantilever.py",
        "modelPath": "model/",
        "ndm": 3,
        "dofMap": "1,2,3"
      }
    },
    "EDP": {
      "Application": "UserDefinedEDP_R",
      "ApplicationData": {
        "EDPspecs": "EDP_specs.json"
      }
    },
    "Simulation": {
       "Application": "OpenSeesPy-Simulation",
       "ApplicationData": {
       }
    },
    "UQ": {
       "Application": "Dakota-UQ",
       "ApplicationData": {
          "method": "LHS",
          "samples": 5,
          "type": "UQ",
          "concurrency": 1,
          "keepSamples": true
       }
    },
    "DL": {
            "Application": "pelicun",
            "ApplicationData": {
                "DL_Method": "HAZUS MH EQ",
                "Realizations": 5,
                "detailed_results": false,
                "log_file": true,
                "coupled_EDP": true,
                "event_time": "off",
                "ground_failure": false
            }
        }
    }
}
